    <div id="stockTable">

        <table border="1">
        <thead>
            <tr><th>Entrada</th><th>Ativo</th><th>C / V</th><th>Valor</th><th>Loss</th><th>Objetivo</th><th>Saída</th><th>Último</th><th>Resultado</th><th>%</th></tr>
        </thead>

        <tbody data-bind="foreach: trades">
            <tr data-bind="css: { text: $id }">
                <td data-bind="text: Symbol"></td>
                <td data-bind="text: Strategy"></td>
                <td data-bind="text: SetupTime"></td>
                <td data-bind="text: Type"></td>
                <td data-bind="text: StartValue"></td>
                <td data-bind="text: StopLoss"></td>
                <td data-bind="text: StopGain"></td>
                <td data-bind="text: EntryTime"></td>
                <td data-bind="text: ExitTime"></td>
                <td data-bind="text: ExitValue"></td>
                <td data-bind="text: NetResult"></td>
                <td data-bind="text: Status"></td>
            </tr>
        </tbody>
        
    </table>
    <span id="noTradesToMonitorMessage">Os Samurais da Centraltrader não estão monitorando nenhum trade de momento.</span>
    
